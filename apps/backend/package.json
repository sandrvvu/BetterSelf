{
  "name": "backend",
  "version": "0.0.1",
  "description": "betterself app backend",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint:check": "eslint \"src/**/*.ts\" --max-warnings=0",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "npx typeorm-ts-node-commonjs migration:create ./src/db/migrations/%npm_config_name%",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate ./src/db/migrations/%npm_config_name% -d ./src/db/typeorm.config.ts",
    "migration:show": "npx typeorm-ts-node-commonjs migration:show -d ./src/db/typeorm.config.ts",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d ./src/db/typeorm.config.ts",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d ./src/db/typeorm.config.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^11.0.3",
    "@types/multer": "^1.4.12",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "joi": "^17.13.3",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "path": "^0.12.7",
    "pg": "^8.13.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^6.21.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.8.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-unicorn": "^56.0.1",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20"
  }
}
